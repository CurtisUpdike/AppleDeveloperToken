<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>AppleDeveloperToken</PackageId>
    <Version>1.0.1</Version>
    <PackageReleaseNotes>Initial Version</PackageReleaseNotes>
    <Title>AppleDeveloperToken</Title>
    <Description>Generate developer tokens for Apple Music API.</Description>
    <Authors>Curtis Updike</Authors>
    <PackageProjectUrl>https://github.com/curtisupdike/AppleDeveloperToken</PackageProjectUrl>
    <RepositoryUrl>https://github.com/curtisupdike/AppleDeveloperToken</RepositoryUrl>
    <PackageTags>apple music token musickit</PackageTags>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="6.27.0" />
  </ItemGroup>

</Project>
